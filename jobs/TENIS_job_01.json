{
  "name": "RUN_PYTHON_SINGLE_NODE",
  "tasks": [
    {
      "task_key": "etl_task",
      "notebook_task": {
        "notebook_path": "/Shared/notebook/py/archivopython.py",
        "base_parameters": {
          "input_path": "dbfs:/FileStore/data/",
          "output_path": "dbfs:/FileStore/jobs_output/"
        }
      },
      "new_cluster": {
        "spark_version": "16.4.x-scala2.12",
        "node_type_id": "Standard_B1ls",
        "driver_node_type_id": "Standard_B1ls",
        "num_workers": 0,
        "is_single_node": true,
        "runtime_engine": "PHOTON",
        "libraries": [
          { "pypi": { "package": "pandas" } },
          { "pypi": { "package": "numpy" } },
          { "pypi": { "package": "pyyaml" } }
        ],
        "kind": "CLASSIC_PREVIEW"
      },
      "timeout_seconds": 3600
    }
  ],
  "max_concurrent_runs": 1
}
